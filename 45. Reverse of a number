'''
Reverse of a number
'''

import java.util.*;
public class Main {
	
	public static void main(String[] args) {
		Scanner S=new Scanner(System.in);
        int n=S.nextInt();
        int rev=0;
        while(n>0){
            int dig=n%10;
            rev=rev*10+dig;
            n=n/10;
        }
        System.out.println(rev);
	}
}
