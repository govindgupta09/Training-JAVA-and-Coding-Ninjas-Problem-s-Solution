'''
Nth Fibonacci Number
'''


import java.util.*;
public class Solution {
    
     static int fib(int n) {
         if (n <= 1) 
             return n; 
         return fib(n-1) + fib(n-2);
     } 
    
	public static void main(String[] args) {
        
		Scanner inp= new Scanner(System.in);
        int n=inp.nextInt();
        int x=fib(n);
        System.out.println(x);
        
	}
}
