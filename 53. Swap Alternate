'''
Swap Alternate
'''

public class Solution {
    
    public static void swapAlternate(int arr[]) {
    	for(int i=0;i<arr.length-1;i+=2){
            int temp=arr[i];
            arr[i]=arr[i+1];
            arr[i+1]=temp;
        }
    }
    
    public static void main(String[] args) {
		int[] arr ={1,34,5,6,57,90,14,10,11,22};
		swapAlternate(arr);
		System.out.print(arr);
	}
}
